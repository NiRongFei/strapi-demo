{
  "connection": "default",
  "collectionName": "news",
  "info": {
    "name": "new",
    "description": "新闻"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "required": true,
      "type": "string"
    },
    "time": {
      "required": true,
      "type": "date"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "date": {
      "type": "date"
    }
  }
}